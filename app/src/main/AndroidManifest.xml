<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.hisp.dhis.android.dashboard">

    <application
        android:name=".DashboardApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.Base">

        <activity android:name="org.hisp.dhis.client.sdk.ui.bindings.views.DefaultLauncherActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="org.hisp.dhis.client.sdk.ui.bindings.views.DefaultLoginActivity" />
        <activity
            android:name=".views.activities.HomeActivity"
            android:theme="@style/AppTheme.Main" />

        <activity
            android:name="org.hisp.dhis.android.dashboard.views.activities.DashboardElementDetailActivity"
            android:label="@string/title_activity_dashboard_item_detail" />

        <meta-data
            android:name="io.fabric.ApiKey"
            android:value="${fabric_api_key}" />
    </application>

</manifest>
